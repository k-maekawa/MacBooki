<div class="main users-new">
  <div class="container">
    
    <div class="form users-from">
      <div class="form-body">
        <% if @error_message %>
        <div class="from-error">
          <%= @error_message %>
        </div>
        <% end %>
        <%= form_tag("/login") do %>
        <p>メールアドレス</p>
        <input name="email" value="<%= @email %>">
        <p>パスワード</p>
        <input type="password" name="password" value="<%= @password %>">
        <input type="submit" value="ログイン">


        <% end %>

      </div>
    </div>
  </div>
</div>
